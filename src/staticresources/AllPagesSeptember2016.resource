var medRecRequestModel = models.MedRecRequest;\nvar request = medRecRequestModel.getFirstRow();\n\nshipmentModel.save({callback: function(){\n\n    var shipment = shipmentModel.getFirstRow();\n\n    if (shipment.Status__c == 'Received' &amp;&amp; shipment.From__c == 'MRHolder') {\n        medRecRequestModel.updateRow(request, {\n            'Status__c' : 'Rcvd'\n    })}\n    \n    else if (shipment.Status__c == 'Sent' &amp;&amp; shipment.From__c == 'MRHolder') {\n        medRecRequestModel.updateRow(request, {\n            'Status__c' : 'In Transit'\n    })}\n    \n    medRecRequestModel.save();\n    \n}});\n\t\n\twindow.history.back();\n</jsitem><jsitem location=\"inlinesnippet\" name=\"GoToStep2\" cachelocation=\"false\">var params = arguments[0],\n\t$ = skuid.$;\n\t\nvar wizard = skuid.component.getById('ViewEditShipmentWizard');\nvar currentStep = wizard.steps[wizard.currentstep];\n\ncurrentStep.navigate('ViewEditStep2');</jsitem></javascript><css/></resources><styles><styleitem type=\"background\" bgtype=\"none\"/></styles></skuidpage>","skuid__Layout2__c":null,"skuid__Layout3__c":null,"skuid__Layout4__c":null,"skuid__Layout5__c":null}]