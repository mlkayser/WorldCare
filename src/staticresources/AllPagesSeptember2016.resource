ipment\n    var shipment = shipmentModel.getFirstRow();\n\n            AvailOutboundMRHolderModel.setCondition(OutboundMRHolderShipTo, shipment.Ship_To__r.Id);\n            AvailOutboundMRHolderModel.activateCondition(OutboundMRHolderShipTo);\n            AvailOutboundMRHolderModel.setCondition(OutboundMRHolderReturnToAccount, shipment.Ship_To__r.Id);\n            AvailOutboundMRHolderModel.activateCondition(OutboundMRHolderReturnToAccount);\n            AvailOutboundMRHolderModel.updateData();\n            \n            InboundfromHospital.setCondition(InboundfromHospitalCondition, shipment.Ship_From__r.Id);\n            InboundfromHospital.activateCondition(InboundfromHospitalCondition);\n            InboundfromHospital.updateData();\n            \n            OutboundToHospital.setCondition(OutboundToHospitalCondition, shipment.Ship_To__r.Id);\n            OutboundToHospital.activateCondition(InboundfromHospitalCondition);\n            OutboundToHospital.updateData();\n            \n        \n}});</jsitem><jsitem location=\"inlinesnippet\" name=\"CreateShippingRecordsOutboundtoMRHolder\" cachelocation=\"false\">var params = arguments[0];\nvar $ = skuid.$;\nvar records = skuid.$.map(arguments[0].list.getSelectedItems(),function(item){ \n    return item.row.Id; \n    });\n    \nvar wizard = $('.nx-wizard, .wizard').data('object');\nvar currentStep = wizard.steps[wizard.currentstep];\n    \nvar models = skuid.model.map();\nvar medrecs = models.AvailMedRecs_OutboundtoMRHolder;\nvar ShipmentModel = models.Shipment;\nvar TheShipment = ShipmentModel.getFirstRow();\nvar shippingrecords = models.ShippingRecords;\n\nShipmentModel.save({callback:function(){\n    var Id = TheShipment.Id;\n\nfor(var n in records){\n    var row = medrecs.getRowById(records[n]);\n\n    if(row !== false){\n        var newRow = shippingrecords.createRow({\n            additionalConditions: [\n                { field: 'Shipment__c', value: Id },\n                { field: 'Medical_Record__c', value: row.Id },\n                { field: 'Patient__c', value: row.MRN__c },\n                { field: 'Case__c', value: row.Case__c },\n                { field: 'Update_MedRec_Status__c', value: 'TRUE' }\n            ]\n        });\n    }\n}\n    shippingrecords.save({callback: function(result){\n    if (result.totalsuccess) {\n       \n    currentStep.navigate('NewShipmentStep4');\n       \n   }}});\n\n}});</jsitem><jsitem location=\"inlinesnippet\" name=\"CreateShippingRecordsOutboundtoHospital\" cachelocation=\"false\">var params = arguments[0];\nvar $ = skuid.$;\nvar records = skuid.$.map(arguments[0].list.getSelectedItems(),function(item){ \n    return item.row.Id; \n    });\n    \nvar wizard = $('.nx-wizard, .wizard').data('object');\nvar currentStep = wizard.steps[wizard.currentstep];\n\nvar models = skuid.model.map();\nvar medrecs = models.AvailMedRecs_OutboundtoHospital;\nvar ShipmentModel = models.Shipment;\nvar TheShipment = ShipmentModel.getFirstRow();\nvar shippingrecords = models.ShippingRecords;\n\nShipmentModel.save({callback:function(){\n    var Id = TheShipment.Id;\n\nfor(var n in records){\n    var row = medrecs.getRowById(records[n]);\n\n    if(row !== false){\n        var newRow = shippingrecords.createRow({\n            additionalConditions: [\n                { field: 'Shipment__c', value: Id },\n                { field: 'Medical_Record__c', value: row.Id },\n                { field: 'Patient__c', value: row.MRN__c },\n                { field: 'Case__c', value: row.Case__c },\n                { field: 'Update_MedRec_Status__c', value: 'TRUE' }\n            ]\n        });\n    }\n}\n\n    shippingrecords.save({callback: function(result){\n    if (result.totalsuccess) {\n       \n    currentStep.navigate('NewShipmentStep4');\n       \n   }}});\n\n}});</jsitem><jsitem location=\"inlinesnippet\" name=\"CreateShippingRecordsInboundfromHospital\" cachelocation=\"false\">var params = arguments[0];\nvar $ = skuid.$;\nvar records = skuid.$.map(arguments[0